{
    "extends": "airbnb-base",
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true,
        "mocha": true
    },
    "ecmaFeatures": {
        "forOf": true,
        "es6": true
    },
    "rules": {
        "no-underscore-dangle": 0,
        "no-param-reassign": 0,
        "import/no-extraneous-dependencies" : 0,
        "prefer-arrow-callback": 0,
        "func-names": 0,
        "no-plusplus": 0,
    }
}
